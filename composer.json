{
    "name": "weirdo/dynamic-acl",
    "authors": [
        {
            "name": "Angel Hidalgo",
            "email": "aghabrego@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework":"9.*|10.*",
        "javoscript/laravel-macroable-models": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DynamicAcl\\Providers\\DynamicAclServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.3",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "DynamicAcl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DynamicAclTest\\" : "tests/"
        }
    }
}
